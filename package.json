{
  "name": "local-logger",
  "displayName": "Local Logger",
  "description": "Logs VS Code activity locally.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaurabh-chakraborty/local-logger-extension"
  },
  "publisher": "GaurabhChakraborty",
  "license": "MIT",
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:my-logger.processLogs",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-logger.processLogs",
        "title": "Process Daily Logs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37"
  }
}
